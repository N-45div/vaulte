{
    "version": "1",
    "name": "vaulte",
    "abis": {
      "Router": {
        "path": "./Router.json"
      }
    },
    "chains": ["ethena-testnet"],
    "instances": [
        {
            "abi": "Router",
            "address": "0x595CeE49356f6260D9D3290f7C052183260314ef",
            "chain": "ethena-testnet",
            "startBlock": 1539807,
            "enrich": {
                "options": {
                  "imports": ["BigDecimal"]
                },
                "handlers": {
                    "Subscription(indexed uint256,address,address,uint256,uint256)": {
                        "entities": {
                            "Subscription": {
                                "userAccount": "event.params.userAccount.toHexString()",
                                "merchantAccount": "event.params.merchantAccount.toHexString()",
                                "tier": "event.params.tier",
                                "subscriptionTime": "event.params.subTime",
                                "timestamp": "event.params.timestamp"
                            }
                        }
                    },
                    "SubscriptionPayment(indexed uint256,address,address,uint256,uint256)": {
                        "entities": {
                            "Payment": {
                                "userAccount": "event.params.userAccount.toHexString()",
                                "merchantAccount": "event.params.merchantAccount.toHexString()",
                                "amount": "event.params.paymentAmount",
                                "paymentTime": "event.params.paymentTime",
                                "timestamp": "event.params.timestamp"
                            }
                        }
                    },
                    "LoanDisbursed(indexed uint256,address,address,uint256,uint8)": {
                        "entities": {
                            "Loan": {
                                "investorAccount": "event.params.investorAccount.toHexString()",
                                "merchantAccount": "event.params.merchantAccount.toHexString()",
                                "amount": "event.params.amount",
                                "loanCategory": "event.params.loanCategory",
                                "timestamp": "event.params.timestamp"
                            }
                        }
                    }
                }
            }
        }
    ]
}